@model IEnumerable<HumanResources.Domain.Entities.JobTitleEntity>

@{
    ViewBag.Title = "Jobs";
}

<h2>Jobs</h2>

<div class="row">
    <div class="col-md-4">
        <ul class="list-group mt-3" id="dropList">
            @foreach (var selectListItem in Model)
            {
                <li class="list-group-item" value="@selectListItem.EntityId">@selectListItem.Name</li>
            }
        </ul>
    </div>
    <div class="col-md-7" id="promotion"></div>
</div>
